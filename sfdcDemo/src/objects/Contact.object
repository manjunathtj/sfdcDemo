        <protected>false</protected>
        <url>{!requireScript(&quot;/soap/ajax/22.0/connection.js&quot;)}
{!requireScript(&quot;/soap/ajax/22.0/apex.js&quot;)} 
var contactID = &apos;{!Contact.Id}&apos;; 
var contactEmail = &apos;{!Contact.Email}&apos;;

try{ 
   var query = &quot;SELECT Id, Name from User where email = &apos;&quot;+contactEmail+&quot;&apos; limit 1&quot;; 
alert(&apos;1111 - query &apos; +query)
   var records = sforce.connection.query(query); 
alert(&apos;222 - records&apos; +records)
   var records1 = records.getArray(&apos;records&apos;); 
alert(&apos;333 records1 - &apos; +records1)
var size = records.size;
alert(&apos;Size - &apos; +size)

  if(records.size == 1){
     alert (&quot;User already exist for the Contact&quot;);
   } else if(contactID != &apos;&apos;) { 
      alert(&quot;Test&quot;);
      parent.location.href =&quot;/apex/CreateUser?ID=&quot;+contactID;
   } else { 
